package com.javarush.test.level05.lesson07.task03;

/* Создать класс Dog
Создать класс Dog (собака) с тремя инициализаторами:
- Имя
- Имя, рост
- Имя, рост, цвет
*/

public class Dog
{
    private String name;
    private int depth;
    private String colour;

    public void initialize(String name)
    {
        this.name = name;
    }

    public void initialize(String name, int depth)
    {
        this.name = name;
        this.depth = depth;
    }

    public void initialize(String name, int depth, String colour)
    {
        this.name = name;
        this.depth = depth;
        this.colour = colour;
    }

}
